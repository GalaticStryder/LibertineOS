	EnableSequence
		'QUAT_MI2S_RX Audio Mixer MultiMedia5':1:1
	EndSequence

	DisableSequence
		'QUAT_MI2S_RX Audio Mixer MultiMedia5':1:0
	EndSequence

	# ALSA PCMs
	CapturePCM 15
	PlaybackPCM 15

EndSection


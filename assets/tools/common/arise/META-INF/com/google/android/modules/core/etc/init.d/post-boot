
#!/system/bin/sh
#
# ARISE Sound Systemsâ„¢
#
# post-boot script
#
# www.arisesound.com


ISS=/system/bin/iss_daemon
LOG=/data/local/post-boot.log
START="Starting iss_daemon...\n"
END="post-boot complete.\n"


(
    echo $START > $LOG
    $ISS
)&


sleep 2


echo $END >> $LOG


mount -o ro,remount /
mount -o ro,remount /system
mount -o ro,remount /data


exit 0

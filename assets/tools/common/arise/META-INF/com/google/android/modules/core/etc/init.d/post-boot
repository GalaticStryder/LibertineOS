
#!/system/bin/sh
#
# ARISE Sound Systemsâ„¢
#
# post-boot script


DTS=/system/bin/dts_hpx_service
ISS=/system/bin/iss_daemon
LOG=/data/local/post-boot.log
START="Starting dts_hpx_service...\n"
END="post-boot complete.\n"


if [ -e "$DTS" ]; then

  (
      $DTS
  )&

fi


echo $START > $LOG


(
    $ISS
)&


sleep 2


echo $END >> $LOG


exit 0

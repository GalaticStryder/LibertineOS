fi

if [ "$UNINSTALLESIRA" ]; then

  ui_print "Uninstalling Esira™ media player module..."
  ui_print " "
  ui_print " "
  sleep 2

  rm -rf $PAPP/Esira
  rm -f $APP/Esira.apk

fi

if [ "$UNINSTALLBEATS" ]; then

  ui_print "Uninstalling ToolBox™ module..."
  ui_print " "
  ui_print " "
  sleep 2

  rm -rf $ALSA
  rm -rf $ETC/srs
  rm -f $ETC/asound.conf
  rm -f $BIN/alsa_amixer
  rm -f $BIN/alsa_aplay
  rm -f $BIN/alsa_ctl
  rm -f $BIN/arm-linux-gnueabi-sndfile-resample
  rm -f $LIB/libalsa-msm.so
  rm -f $LIB/libasound.so
  rm -f $LIB/libresample.so
  rm -f $LIB/libsamplerate.so
  rm -f $LIB/libsndfile.so
  rm -f $SUD/smeejaytee_setprop
  rm -f $INITD/smeejaytee_setprop

  mkdir -p $ALSA
  mkdir -p $ETC/srs
  cp -Rf core/usr/share/alsa/* $ALSA
  cp -Rf custom/huawei/system/etc/srs/* $ETC/srs
  cp -f $CORE/etc/init.d/$PROP $INITD/$PROP
  $SPR $ALSA 0 0 0755 0644
  $SPR $ETC/srs 0 0 0755 0644
  $SP $INITD/$PROP 0 0 0700
  restorecon $INITD/$PROP

  if [ -e "$ACCBAK" ]; then

    rm -f $ACC
    mv -f $ACCBAK $ACC
    $SP $ACC 0 0 0644

  fi

fi

if [ "$UNINSTALLXHIFI" ]; then

  ui_print "Uninstalling ViPER4Android XHiFi module..."
  ui_print " "
  ui_print " "
  sleep 2

  rm -rf $PAPP/$XHIFI
  rm -f $APP/$XHIFIAPK
  rm -f $DEUTFX/$XHIFILIB

  for CFG in $ETC/$AEC $ETC/$HTC $ETC/$AEV $VETC/$AOE; do

    if [ -f "$CFG" ]; then

      sed -i '/v4a_xhifi {/,/}/d' $CFG
      sed -i '/v4a_standard_xhifi {/,/}/d' $CFG

    fi

  done

fi

if [ "$UNINSTALLMFX" ]; then

  ui_print "Uninstalling $MFX..."
  ui_print " "
  ui_print " "
  sleep 2

  rm -rf $PAPP/$MFX
  rm -rf $APP/$MFX
  rm -f $PAPP/$MFXAPK
  rm -f $APP/$MFXAPK

fi

if [ "$UNINSTALLDIRAC" ]; then

  ui_print "Uninstalling Dirac™ Power Sound module..."
  ui_print " "
  ui_print " "
  sleep 2

  rm -rf $APP/$DACS
  rm -rf $APP/$OPDIR
  rm -f $APP/$DACSAPK
  rm -f $APP/$OPDIRAPK
  rm -f $DEUTFX/libdirac.so
  rm -f $DEUTFX64/libdirac.so
  rm -f $ETC/diracmobile.config
  rm -f $ETC/diracvvd.bin
  rm -f $LIB/libDiracAPI_SHARED.so
  rm -f $LIB64/libDiracAPI_SHARED.so
  rm -f $PERM/com.oppo.features_dirac.xml

  for CFG in $ETC/$AEC $ETC/$HTC $ETC/$AEV $VETC/$AOE; do

    if [ -f "$CFG" ]; then

      sed -i '/dirac {/,/}/d' $CFG
      sed -i '/dirac_controller {/,/}/d' $CFG
      sed -i '/dirac_music {/,/}/d' $CFG

    fi

  done

fi

if [ "$UNINSTALLDTS" ]; then

  ui_print "Uninstalling $DTS module..."
  ui_print " "
  ui_print " "
  sleep 2

  rm -rf $PAPP/$DTS
  rm -f $APP/$DTSAPK

fi

rm -rf $ARISE

if [ ! -d "$SYS/vendor" ] || [ -L "$SYS/vendor" ]; then

  umount /vendor

fi

ui_print " "
ui_print " "
ui_print "This session is now complete."
ui_print " "
ui_print " "

umount /system

ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print "               www.arisesound.com"
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "
ui_print " "

sleep 2

exit 0

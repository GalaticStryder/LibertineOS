mkdir $ARISE
cd $ARISE
unzip -o "$ZIP"

ui_print "200MB - 300MB of free /system space is necessary for successful installations of various module combinations."
ui_print " "
ui_print " "
sleep 3
ui_print "Various scripts in the Core and other modules depend heavily on SuperSU, systemless or system, for specific operations during and after boot."
ui_print " "
ui_print " "
sleep 3
ui_print "STRONGLY consider using SuperSU for all root management needs if our sound enhancement solutions are part of your Android experience."
ui_print " "
ui_print " "
sleep 3
ui_print "The dependency on SuperSU for achieving the full, intended experience is not one which is satisfactory for us. Achieving the full, intended experience without root is always a priority."
ui_print " "
ui_print " "
sleep 5
ui_print " "
ui_print " "
ui_print "Obtaining instructions from $WISHLIST..."
ui_print " "
ui_print " "
ui_print " "
ui_print " "
sleep 3

if [ ! -e "$WISHLIST" ]; then

  ui_print "$WISHLIST not found!"
  ui_print " "
  ui_print " "
  ui_print "Creating $WISHLIST for you..."
  cp -f getting_started/your_wishlist/arise_customize.prop $WISHLIST
  ui_print " "
  ui_print " "
  ui_print "Reboot, add values to $WISHLIST, and try again."
  ui_print " "
  ui_print " "
  ui_print "Detailed information and instructions can be found in /getting_started inside the archive."
  sleep 4
  exit 1

fi

if [ ! "$INSTALLCORE" ] && [ ! -e "$INFO" ]; then

  ui_print "$INFO not found!"
  ui_print " "
  ui_print " "
  ui_print "This modular installer requires the Core module to be installed or to be already installed."
  ui_print " "
  ui_print " "
  sleep 4
  ui_print "Reboot, add values to $WISHLIST, and try again."
  ui_print " "
  ui_print " "
  ui_print "Once the Core module is installed, the following line in $WISHLIST should be edited to remove the value of "true" for future customization without installing the Core module again."
  ui_print " "
  ui_print " "
  ui_print ""install.core=true""
  ui_print " "
  ui_print " "
  ui_print "Detailed information and instructions can be found in /getting_started inside the archive."
  sleep 4
  exit 1


if [ "$BUFFER" ]; then

  ui_print " "
  ui_print " "
  ui_print "Removing deep_buffer from audio_policy..."
  ui_print " "
  ui_print " "

  if [ -f "$ETC/$POL" ]; then

    if [ -e "$ETC/$POLBAK" ]; then

      sed -i '/deep_buffer {/,/}/d' $ETC/$POL
      sed -i '/raw {/,/}/d' $ETC/$POL

    else

      cp -f $ETC/$POL $ETC/$POLBAK
      $SP $ETC/$POLBAK 0 0 0644
      sed -i '/deep_buffer {/,/}/d' $ETC/$POL
      sed -i '/raw {/,/}/d' $ETC/$POL

    fi

  fi

  if [ -f "$ETC/$AOP" ]; then

    if [ -e "$ETC/$AOPBAK" ]; then

      sed -i '/deep_buffer {/,/}/d' $ETC/$AOP
      sed -i '/raw {/,/}/d' $ETC/$AOP

    else

      cp -f $ETC/$AOP $ETC/$AOPBAK
      $SP $ETC/$POLBAK 0 0 0644
      sed -i '/deep_buffer {/,/}/d' $ETC/$AOP
      sed -i '/raw {/,/}/d' $ETC/$AOP

    fi

  fi

  if [ -f "$VETC/$POL" ]; then

    if [ -e "$VETC/$POLBAK" ]; then

      sed -i '/deep_buffer {/,/}/d' $VETC/$POL
      sed -i '/raw {/,/}/d' $VETC/$POL

    else

      cp -f $VETC/$POL $VETC/$POLBAK
      $SP $VETC/$POLBAK 0 0 0644
      sed -i '/deep_buffer {/,/}/d' $VETC/$POL
      sed -i '/raw {/,/}/d' $VETC/$POL

    fi

  fi

  if [ "$OP3" ]; then

    if [ -f "$ETC/$APC" ]; then

      if [ -e "$ETC/$APCBAK" ]; then

        sed -i '/Speaker/{n;s/deep_buffer,//;}' $ETC/$APC

      else

        cp -f $ETC/$APC $ETC/$APCBAK
        $SP $ETC/$APCBAK 0 0 0644
        sed -i '/Speaker/{n;s/deep_buffer,//;}' $ETC/$APC

      fi

    fi

  else

    if [ -f "$ETC/$APC" ]; then

      if [ -e "$ETC/$APCBAK" ]; then

        sed -i 's/deep_buffer,//g' $ETC/$APC
        sed -i 's/,deep_buffer,//g' $ETC/$APC
        sed -i 's/,deep_buffer//g' $ETC/$APC

      else

        cp -f $ETC/$APC $ETC/$APCBAK
        $SP $ETC/$APCBAK 0 0 0644
        sed -i 's/deep_buffer,//g' $ETC/$APC
        sed -i 's/,deep_buffer,//g' $ETC/$APC
        sed -i 's/,deep_buffer//g' $ETC/$APC

      fi

    fi

  fi

fi

if [ "$PERMISSIVE" ]; then

  ui_print " "
  ui_print " "
  ui_print "Setting SELinux Permissive Mode via script..."
  ui_print " "
  ui_print " "

  rm -f $SUD/$SERVER
  cp -f $CSUD/permissive $SUD/permissive
  $SP $SUD/permissive 0 0 0700
  restorecon $SUD/permissive

fi

if [ "$PERMISSIONS" ]; then

  if [ -d "$SYS/lib64" ]; then
    LIB=$SYS/lib64
  else
    LIB=$SYS/lib
  fi

  set_perm() {
    uid=$1; gid=$2; mod=$3;
    shift 3;
    chown $uid.$gid $*; chown $uid:$gid $*;
    chmod $mod $*;
  }

  set_perm_recursive() {
    uid=$1; gid=$2; dmod=$3; fmod=$4;
    shift 4;
    until [ ! "$1" ]; do
      chown -R $uid.$gid $1; chown -R $uid:$gid $1;
      find "$1" -type d -exec chmod $dmod {} +;
      find "$1" -type f -exec chmod $fmod {} +;
      shift;
    done;
  }

  restore_con() { 
    for i in $SYS/bin/toybox $SYS/toolbox $SYS/bin/toolbox; do
      LD_LIBRARY_PATH=$LIB $i restorecon -R $*
    done
    restorecon -R $*
  }

  set_perm_recursive 1023 1023 0775 0664 "/data/media"
  set_perm 1023 1023 0770 "/data/media" "/data/media/0"
  restore_con /data/media/0

fi


    $SP $LIB/$FFMPGVID 0 0 0644
    $SP $LIB/$FFMPGAUDHW 0 0 0644
    $SP $LIB/$FFMPGVIDHW 0 0 0644
    $SP $LIB/$SWS 0 0 0644
    $SP $LIB/$SWS2 0 0 0644
    $SP $LIB/$SWS3 0 0 0644
    $SP $PERM/com.huawei.audioalgo.xml 0 0 0644
    ln -s $SYS/framework $VEN/framework

    if [ ! -d "$VEN/firmware" ]; then

      mkdir -p $VEN/firmware
      $SP $VEN/firmware 0 2000 0755

    fi

    ln -s $ETC/firmware/hifi_6402.img $VEN/firmware/hifi_6402.img
    ln -s $ETC/firmware/hifi_6402_2spk.img $VEN/firmware/hifi_6402_2spk.img
    ln -s $ETC/firmware/hifi_6403.img $VEN/firmware/hifi_6403.img
    ln -s $ETC/firmware/hifi_6403_tfa.img $VEN/firmware/hifi_6403_tfa.img
    ln -s $ETC/firmware/isp_dts.img $VEN/firmware/isp_dts.img
    rm -f $VEN/lib/libaudiodevarb.so
    ln -s $LIB/libaudiodevarb.so $VLIB/libaudiodevarb.so

  elif [ ! "$QCOM" ]; then

    cp $CAFX/huawei/$AEC $ETC/$AEC

  fi

  for QCFX in $INSTALLQCAO $INSTALLQCAS $INSTALLQCHW $INSTALLQCSS $INSTALLQCSO $INSTALLQCSK; do

    if [ "$QCFX" ]; then

      if [ ! "$QCOM" ]; then

        ui_print "Qualcomm™ not detected!"
        ui_print " "
        ui_print " "
        ui_print "The appropriate audio effects configuration already in place will be left as is."
        sleep 3

      elif [ "$INSTALLQCAO" ]; then

        ui_print "Installing Android O Qualcomm audio effects configuration..."

        cp -f $CAFX/qc/android_o/$AEC $ETC/$AEC
        $SP $ETC/$AEC 0 0 0644

      elif [ "$INSTALLQCAS" ]; then

        ui_print "Installing Asus™ Qualcomm™ audio effects configuration..."
        sleep 2

        cp -f $CAFX/qc/asus/$AEC $ETC/$AEC
        $SP $ETC/$AEC 0 0 0644

      elif [ "$INSTALLQCHW" ]; then

        ui_print "Installing Huawei™ Qualcomm™ audio effects configuration..."
        sleep 2

        cp -f $CAFX/qc/huawei/$AEC $ETC/$AEC
        $SP $ETC/$AEC 0 0 0644

      elif [ "$INSTALLQCSO" ]; then

        ui_print "Installing Sony™ Qualcomm™ audio effects configuration..."
        sleep 2

        cp -f $CAFX/qc/sony/$AEC $ETC/$AEC
        $SP $ETC/$AEC 0 0 0644

      elif [ "$INSTALLQCSK" ]; then

        ui_print "Installing Stock Qualcomm™ audio effects configuration..."
        sleep 2

        cp -f $CAFX/qc/stock/$AEC $ETC/$AEC
        $SP $ETC/$AEC 0 0 0644

      fi

      ui_print " "
      ui_print " "

    fi

  done

  if [ -e "$ARISEFX" ]; then

    ui_print "$ARISEFX found and will be used..."
    ui_print " "
    ui_print " "
    sleep 2

    cp -f $ARISEFX $ETC/$AEC
    $SP $ETC/$AEC 0 0 0644


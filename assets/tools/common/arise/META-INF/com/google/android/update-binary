  $SP $LIB/libUDT.so 0 0 0644
  $SP $LIB/libvad.so 0 0 0644
  $SP $LIB/libval_2.0.so 0 0 0644
  $SP $LIB/libval_common.so 0 0 0644
  restorecon $SUD/$PROP

  if [ -e "$SDCARD/$PROP" ]; then


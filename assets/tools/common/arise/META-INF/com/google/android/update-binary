  fi

  if [ "$X86" ]; then

    cp -f $CLIBX86/$APSS $LIB/$APSS
    cp -f $CLIBX86/$FREQ $LIB/$FREQ
    cp -f $CLIBX86/$LATENCY $LIB/$LATENCY

  for CFG in $ETC/$AEC $ETC/$HTC $ETC/$AEV $VETC/$AOE; do

    if [ -f "$CFG" ]; then

      sed -i '/am3daudioenhancement {/,/}/d' $CFG

    fi

  done

fi

if [ "$UNINSTALLSONY" ]; then

  ui_print " "
  ui_print " "
  ui_print "Uninstalling Sony™ Music Suite module..."
  ui_print " "
  ui_print " "

  rm -rf $APP/$DLNA
  rm -rf $APP/$KARAOKE
  rm -rf $APP/$LYRIC
  rm -rf $APP/$SOUND
  rm -rf $APP/$UNHSNTF
  rm -rf $APP/$WIKI
  rm -rf $APP/$YTUBE
  rm -rf $PAPP/$MUSIC
  rm -rf $PAPP/$POD
  rm -rf $PAPP/$TRKID
  rm -rf $ETC/sony_effect
  rm -f $ETC/$DSX
  rm -f $ETC/media_codecs_somc.xml
  rm -f $APP/$DLNAPK
  rm -f $APP/$KARAOKEAPK
  rm -f $APP/$LYRICAPK
  rm -f $APP/$MUSICAPK
  rm -f $APP/$PODAPK
  rm -f $APP/$SOUNDAPK
  rm -f $APP/$TRKIDAPK
  rm -f $APP/$UNHSNTF
  rm -f $APP/$WIKIAPK
  rm -f $APP/$YTUBEAPK
  rm -f $LIB/$DSEE
  rm -f $LIB/libdlnacommonjni.so
  rm -f $LIB/libdlnaplayer.so
  rm -f $LIB/$LDAC
  rm -f $LIB/libhscomp.so
  rm -f $LIB/libhscomp_jni.so
  rm -f $LIB/libhuey.so
  rm -f $LIB/libhuey_dms_plugin.so
  rm -f $LIB/libprotobuf-cpp-2.3.0-lite.so
  rm -f $LIB64/$DSEE
  rm -f $LIB64/libdlnacommonjni.so
  rm -f $LIB64/libdlnaplayer.so
  rm -f $LIB64/libhscomp.so
  rm -f $LIB64/libhscomp_jni.so
  rm -f $LIB64/libhuey.so
  rm -f $LIB64/libhuey_dms_plugin.so
  rm -f $LIB64/libprotobuf-cpp-2.3.0-lite.so
  rm -f $SUD/sony_setprop

fi

if [ "$UNINSTALLESIRA" ]; then

  ui_print " "
  ui_print " "
  ui_print "Uninstalling Esira™ media player module..."
  ui_print " "
  ui_print " "

  rm -rf $PAPP/Esira
  rm -f $APP/Esira.apk

fi

if [ "$UNINSTALLBEATS" ]; then

  ui_print " "
  ui_print " "
  ui_print "Uninstalling ToolBox™ module..."
  ui_print " "
  ui_print " "

  rm -rf $ALSA
  rm -rf $ETC/srs
  rm -f $ETC/asound.conf
  rm -f $BIN/alsa_amixer
  rm -f $BIN/alsa_aplay
  rm -f $BIN/alsa_ctl
  rm -f $BIN/arm-linux-gnueabi-sndfile-resample
  rm -f $LIB/libalsa-msm.so
  rm -f $LIB/libasound.so
  rm -f $LIB/libresample.so
  rm -f $LIB/libsamplerate.so
  rm -f $LIB/libsndfile.so
  rm -f $SUD/smeejaytee_setprop

  mkdir -p $ALSA
  mkdir -p $ETC/srs
  cp -Rf core/usr/share/alsa/* $ALSA
  cp -Rf custom/huawei/system/etc/srs/* $ETC/srs
  cp -f core/su.d/$PROP $SUD/$PROP
  $SPR $ALSA 0 0 0755 0644
  $SPR $ETC/srs 0 0 0755 0644
  $SP $SUD/$PROP 0 0 0700
  restorecon $SUD/$PROP

  if [ -e "$ETC/$HTCBAK" ]; then

    rm -f $ETC/$HTC
    mv -f $ETC/$HTCBAK $ETC/$HTC
    $SP $ETC/$HTC 0 0 0644

  fi

  if [ -e "$ACCBAK" ]; then

    rm -f $ACC
    mv -f $ACCBAK $ACC
    $SP $ACC 0 0 0644

  fi

fi

if [ "$UNINSTALLXHIFI" ]; then

  ui_print " "
  ui_print " "
  ui_print "Uninstalling ViPER4Android XHiFi module..."
  ui_print " "
  ui_print " "

  rm -rf $PAPP/$XHIFI
  rm -f $APP/$XHIFIAPK
  rm -f $DEUTFX/$XHIFILIB

  for CFG in $ETC/$AEC $ETC/$HTC $ETC/$AEV $VETC/$AOE; do

    if [ -f "$CFG" ]; then

      sed -i '/v4a_xhifi {/,/}/d' $CFG
      sed -i '/v4a_standard_xhifi {/,/}/d' $CFG

    fi

  done

fi

if [ "$UNINSTALLMFX" ]; then

  ui_print " "
  ui_print " "
  ui_print "Uninstalling MusicFX..."
  ui_print " "
  ui_print " "

  rm -rf $PAPP/$MFX
  rm -rf $APP/$MFX
  rm -f $PAPP/$MFXAPK
  rm -f $APP/$MFXAPK

fi

rm -rf $ARISE

if [ ! -d "$SYS/vendor" ]; then

  umount /vendor

fi

ui_print " "
ui_print " "
ui_print "This session has completed."
ui_print " "
ui_print " "
ui_print ":good:"
ui_print " "
ui_print " "

umount /system

sleep 2

exit 0
